<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="300.0" spacing="10.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bdsqltester.scenes.admin.AssignmentUserGradesController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <Label fx:id="assignmentNameLabel" text="Nilai Pengguna untuk Tugas:" />
    <TableView fx:id="userGradesTableView" prefHeight="300.0">
        <columns>
            <TableColumn fx:id="userIdColumn" prefWidth="75.0" text="ID Pengguna" />
            <TableColumn fx:id="userNameColumn" prefWidth="120.0" text="Nama Pengguna" />
            <TableColumn fx:id="gradeColumn" prefWidth="75.0" text="Nilai" />
        </columns>
    </TableView>
</VBox>
